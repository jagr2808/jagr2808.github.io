@article{GREVSTAD2024107603,
 abstract = {We introduce the concept of an n-minimal Auslander–Gorenstein category and n-precluster tilting subcategory. With this, we create an analog of the higher Auslander–Solberg correspondence (Iyama–Solberg 2018) for exact categories. Our approach is based on the recent generalization of the (higher) Auslander correspondence to exact categories (Henrard–Kvamme–Roosmalen 2020, Ebrahimi–Nasr-Isfahani 2021).},
 author = {Jacob Fjeld Grevstad},
 doi = {https://doi.org/10.1016/j.jpaa.2023.107603},
 issn = {0022-4049},
 journal = {Journal of Pure and Applied Algebra},
 number = {6},
 pages = {107603},
 title = {Higher Auslander–Solberg correspondence for exact categories},
 url = {https://www.sciencedirect.com/science/article/pii/S0022404923002852},
 volume = {228},
 year = {2024}
}
